<%= render 'books/list',user: @user, book: @book %>

<div class="col-md-8 offset-md-1">
        <h2>Users</h2>
        <% @users.each do |user| %>
        <table class="table">
          <thead>
            <tr>
              <th>image</th>
              <th>name</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg', fallback: "no_image.jpg" %></td>
              <td><%= user.name %></td>
              <td><%=link_to "Show", user_path(user.id) %></td>
            </tr>
          </tbody>
          <% end %>
        </table>
      </div>
    </div>
  </div>
</div>
<%= paginate @users %>
